					<Image src={imageThirtyFour} width="1224" height="1584" alt="Untitled"></Image>
					<Image src={imageThirtyFive} width="1224" height="1584" alt="Untitled"></Image>
				</div>
				<!-- omfg txt -->
				<div class="grid grid-cols-1 md:grid-cols-2 gap-8 mb-8">
					<Image src={imageThirtySix} width="2048" height="2048" alt="Untitled"></Image>
