import imageTwentySeven from '../assets/art/Alpha-08@2x.png';
import imageTwentyEight from '../assets/art/Alpha-09b@2x.png';	
import imageTwentyNine from '../assets/art/008.png';
import imageThirty from '../assets/art/009.png';
import imageThirtyOne from '../assets/art/011.png';
import imageThirtyTwo from '../assets/art/012.png';
import imageThirtyThree from '../assets/art/013.png';
import imageThirtyFour from '../assets/art/PlusMinus-01.png';
import imageThirtyFive from '../assets/art/PlusMinus-02.png';
import imageThirtySix from '../assets/art/015.png';
import imageThirtySeven from '../assets/art/016.png';
