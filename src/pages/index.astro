<!doctype html>
<html lang="en">
	<head>
		<BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
	</head>
	<body>
		<Layout>
			<div class="container mx-auto p-8">
				<div class="grid grid-cols-1 gap-8 mb-8">
					<Image src={imageEight} width="4096" height="1714" alt="Untitled"></Image>
				</div>
				<!-- New Art -->
				<div class="grid grid-cols-1 md:grid-cols-1 lg:grid-cols-2 xxl:grid-cols-3 gap-8">
					<Image src={imageOne} width="2048" height="2048" alt="Untitled"></Image>
					<!-- <Image src={imageTwo} width="2048" height="2048" alt="Untitled"></Image> -->
					<Image src={imageFive} width="2048" height="2048" alt="Untitled"></Image>
					<Image src={imageThree} width="2048" height="2048" alt="Untitled"></Image>
					<Image src={imageFour} width="2048" height="2048" alt="Untitled"></Image>
					<Image src={imageSix} width="2048" height="2048" alt="Untitled"></Image>
					<!-- <Image src={imageSeven} width="2048" height="2048" alt="Untitled"></Image> -->
					<!-- <Image src={imageTen} width="2048" height="2048" alt="Untitled"></Image> -->
					<Image src={imageEleven} width="2048" height="2048" alt="Untitled"></Image>
				</div>
				<div class="grid grid-cols-1 gap-8 mt-8 mb-8">
